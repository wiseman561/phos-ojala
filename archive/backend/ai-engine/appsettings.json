{
  "VaultSettings": {
    "Address": "http://vault:8200",
    "Role": "ai-engine-role",
    "MountPath": "phos-secrets",
    "SecretPath": "ai-engine"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=postgres;Database=phos_ai_engine;User Id=postgres;Password=postgres;"
  },
  "Services": {
    "ApiGateway": {
      "Url": "http://phos-apigateway"
    }
  },
  "AIEngine": {
    "ModelSettings": {
      "HealthScoreModelPath": "/app/models/health_score_model.pkl",
      "RiskAssessmentModelPath": "/app/models/risk_assessment_model.pkl",
      "ForecastModelPath": "/app/models/forecast_model.pkl"
    },
    "FeatureImportance": {
      "EnableExplanations": true,
      "MaxFeatures": 10
    },
    "Cache": {
      "Enabled": true,
      "ExpirationMinutes": 60
    }
  }
}
