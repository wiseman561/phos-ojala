{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "YOUR_SUPER_SECRET_KEY_HERE_THAT_IS_AT_LEAST_32_CHARACTERS_LONG",
    "Issuer": "Phos.Identity",
    "Audience": "Phos.Client",
    "ExpiryInMinutes": "60"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=phos-db;Port=5432;Database=phos;Username=postgres;Password=yourpassword",
    "Redis": "phos-redis:6379"
  },
  "FeatureManagement": {
    "AIIntegration": true,
    "AdvancedAnalytics": true,
    "TeleHealth": true,
    "PatientPortal": true
  },
  "VaultSettings": {
    "Url": "http://vault:8200",
    "MountPath": "phos-secrets",
    "SecretPath": "api"
  }
}
