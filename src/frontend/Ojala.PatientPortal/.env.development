# Development Environment Variables
REACT_APP_API_URL=http://localhost:5010
REACT_APP_ENVIRONMENT=development
REACT_APP_APP_NAME=Ojala Patient Portal
REACT_APP_VERSION=1.0.0

# API Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_TOKEN_REFRESH_INTERVAL=900000

# Feature Flags
REACT_APP_ENABLE_TELEMETRY=true
REACT_APP_ENABLE_TELEHEALTH=true
REACT_APP_ENABLE_OMICS=true
REACT_APP_ENABLE_MESSAGING=true

# Debug Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug

# WebSocket Configuration
REACT_APP_WS_URL=ws://localhost:5010/ws
